{
  "context_servers": {
    "XcodeBuildMCP": {
      "command": "npx",
      "args": ["-y", "xcodebuildmcp@latest"],
      "source": "custom"
    },
    "Serena": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server"
      ],
      "source": "custom"
    },
    "Linear": {
      "command": "npx",
      "args": ["-y", "mcp-remote", "https://mcp.linear.app/sse"],
      "source": "custom"
    },
    "GitHub": {
      "command": "npx",
      "args": ["-y", "mcp-remote", "https://api.githubcopilot.com/mcp/"],
      "source": "custom"
    },
    "Figma": {
      "command": "npx",
      "args": ["-y", "mcp-remote", "http://127.0.0.1:3845/sse"],
      "source": "custom"
    },
    "Beeper": {
      "command": "npx",
      "args": ["-y", "mcp-remote", "http://localhost:23373/v0/mcp"],
      "source": "custom"
    },
    "Codex": {
      "command": "codex",
      "args": ["mcp"],
      "source": "custom"
    },
    "ChromeDevtools": {
      "command": "npx",
      "args": ["chrome-devtools-mcp@latest"],
      "source": "custom"
    },
    "Context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp@latest"],
      "source": "custom"
    },
    "skillz": {
      "command": "uvx",
      "args": [
        "skillz@latest",
        "/Users/shunkakinoki/ghq/github.com/shunkakinoki/dotagents/.skillz"
      ],
      "source": "custom"
    }
  }
}
